<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="FloatingMenuButton">
		<attr name="animationType" format="string"/>
		<attr name="dispositionStartAngle" format="integer"/>
		<attr name="dispositionEndAngle" format="integer"/>
		<attr name="subActionButtonRadius" format="integer"/>
		<attr name="anchored" format="boolean"/>
	</declare-styleable>

	<attr name="fromDegrees" format="float|reference" />
	<attr name="toDegrees" format="float|reference" />
	<attr name="childSize" format="dimension|reference" />

	<declare-styleable name="ArcLayout">
		<attr name="fromDegrees"/>
		<attr name="toDegrees"/>
		<attr name="childSize"/>
	</declare-styleable>

	<declare-styleable name="ArcMenu">
		<attr name="fromDegrees"/>
		<attr name="toDegrees"/>
		<attr name="childSize"/>
	</declare-styleable>

</resources>